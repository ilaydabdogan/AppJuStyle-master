<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pro.android.justyle">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/justyle_icon_cut"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/justyle_icon_cut"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.pro.android.justyle.FrontPageActivity" />
        <activity android:name="com.pro.android.justyle.ProfileActivity" />
        <activity android:name="com.pro.android.justyle.LoginActivity"/>
            <activity android:name="com.pro.android.justyle.CreateArticleActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.pro.android.justyle.SignUpActivity" />


    </application>

</manifest>